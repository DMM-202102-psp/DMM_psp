<div class="container">
<div class="row my-3">
  <div class="ml-5 pl-4">
<h3 style="background:#DDDDDD">配送先編集</h3>
<%= form_with model: @address, url: address_path, local:true do |f| %>
 </div>
    </div>
    
    <table>
<tr>
  <th><%= f.label :郵便番号（ハイフンなし）%></th>
  <th><%= f.text_field :postcode %></th>
</tr>

<tr>
  <th><%= f.label :住所 %></th>
 <th> <%= f.text_field :address, size:50 %></th>
</tr>

<tr>
  <th><%= f.label :宛名 %></th>
  <th><%= f.text_field :name %></th>
  <th><%= f.submit "変更内容を保存", class: 'btn btn-success' %></th>
</tr>


</table>
<% end %>