<div class="container">

   <div class="col-xs-1">
     <div class="col-xs-9">


<tr>
  <th>商品一覧
   <span>
    <%= @items.count %>件
   </span>
  </th>
</tr>

<% @items.each do |item| %>


<tr>
  <th><%= attachment_image_tag(@item, :image, format: 'jpeg', size: "50x50", fallback: "no_image.jpg") %>
  </th>
</tr>

<tr>
  <th><%= item.name %></th>
</tr>

<tr>
  <th>¥<%= item.price %></th>
</tr>

<% end %>


   </div>
  </div>
</div>
<%= paginate @items %>