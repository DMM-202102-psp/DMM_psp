<div class="container">

   <div class="col-xs-1">
     <div class="col-xs-9">


<h3>
商品一覧
   <span>
    <%= @items.count %>件
   </span>
</h3>
<% @items.each do |item| %>


<teble class="table">
<thead>
  <tr>
   <th><%= link_to attachment_image_tag(item, :image, format: 'jpeg', size: "150x130", fallback: "no_image.jpg"), item_path(item) %></th>
  </tr>
</thead>

<tbody>
<tr>
  <td><%= item.name %></td>
</tr>

<tr>
  <td>¥<%= item.price %></td>
</tr>

</tbody>
</teble>
<% end %>


   </div>
  </div>
</div>
<%= paginate @items %>