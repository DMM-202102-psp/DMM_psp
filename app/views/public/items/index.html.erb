<div class="container">

   <div class="col-xs-1">
     <div class="col-xs-9">


<h3>
商品一覧
   <span>
    <%= @items.count %>件
   </span>
</h3>
<% @items.each do |item| %>
<teble>
<tr>
  <th>  <%= link_to attachment_image_tag(item, :image, format: 'jpeg', size: "210x200", fallback: "no_image.jpg"), item_path(item) %>
  </th>
</tr>

<tr>
  <th><%= item.name %></th>
</tr>

<tr>
  <th>¥<%= item.price %></th>
</tr>
</teble>
<% end %>


   </div>
  </div>
</div>
<%= paginate @items %>